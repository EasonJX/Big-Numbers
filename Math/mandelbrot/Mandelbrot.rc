        MENUITEM SEPARATOR
        MENUITEM "&Exit",                       ID_FILE_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Calculate image\tF5",        ID_EDIT_CALCULATEIMAGE
        MENUITEM "&Suspend calculation\tAlt+S", ID_EDIT_SUSPENDCALCULATION
        MENUITEM "&Abort calculation\tEsc",     ID_EDIT_ABORTCALCULATION
