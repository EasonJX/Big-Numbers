        MENUITEM "Use &edge detection\tAlt+E",  ID_OPTIONS_USEEDGEDETECTION, CHECKED
        MENUITEM "Use only &1 CPU\tAlt+1",      ID_OPTIONS_USEONLY1CPU
        MENUITEM SEPARATOR
        MENUITEM "&Retain aspect ratio\tAlt+R", ID_OPTIONS_RETAIN_ASPECTRATIO, CHECKED
